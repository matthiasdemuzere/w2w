	keywords = {NUDAPT,Urban boundary layer,WRF BEP/BEM,WUDAPT},
	number = {June 2018},
	pages = {100460},
	publisher = {Elsevier},
	title = {{Evaluation of uWRF performance and modeling guidance based on WUDAPT and NUDAPT UCP datasets for Hong Kong}},
